<h1>Show Connections</h1>

<div class="one-container">
	<p>
	Start: <%= @connection.get_start %> </br>
	End: <%= @connection.get_end %> </br>
	Apartment: <%= @connection.apartment.apartment_name %> </br>
	Landlord: <%= @connection.landlord.full_name %> </br>
	Tenants: </br> </br>
	<% @tenants.each do |tenant| %>
		<%= tenant.full_name %> </br>
		<%= tenant.email %> </br>
		<%= tenant.phone_number %> </br> </br>
	<% end %>
	</p>
</div>
